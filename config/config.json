{
  "ultra_light": {
    "enabled": true,
    "buffer_size": 10000,
    "export_interval_seconds": 30,
    "max_memory_mb": 50,
    "use_ultra_fast_mode": false
  },
  "compaction": {
    "enabled": true,
    "interval_hours": 4,
    "min_files_for_compaction": 10,
    "max_compacted_size_mb": 50
  },
  "retention": {
    "enabled": true,
    "retention_days": 30,
    "cleanup_hours": 1,
    "keep_min_files": 100,
    "max_total_size_gb": 10.0
  },
  "server": {
    "port": "5555",
    "enable_cors": true
  }
}